				<componentref id="cube"/>
			</children>
		</component>


		<component id="elevation">
			<transformation>
				<translate x="0" y="0" z="-21" />
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<componentref id="elevation_back"/>
				<componentref id="elevation_step1"/>
				<componentref id="elevation_step2"/>
				<componentref id="elevation_step3"/>
			</children>
		</component>


		<component id="elevation_back">
			<transformation>
				<translate x="0" y="0.6" z="-2" />
				<scale x="40" y="1.2" z="4" />
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<componentref id="cube"/>
			</children>
		</component>

		<component id="elevation_step1">
			<transformation>
				<translate x="0" y="0.1" z="0" />
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<componentref id="elevation_step"/>
			</children>
		</component>

		<component id="elevation_step2">
			<transformation>
				<translate x="0" y="0.3" z="0" />
				<scale x="0.9" y="1" z="0.9" />
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<componentref id="elevation_step"/>
			</children>
		</component>

		<component id="elevation_step3">
			<transformation>
				<translate x="0" y="0.5" z="0" />
				<scale x="0.8" y="1" z="0.8" />
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<componentref id="elevation_step"/>
			</children>
		</component>

		<component id="elevation_step">
			<transformation>
				<translate x="0" y="0" z="2" />
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<componentref id="elevation_p1"/>
				<componentref id="elevation_p2"/>
			</children>
		</component>


		<component id="elevation_p1">
			<transformation>
				<scale x="40" y="0.2" z="4" />
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<componentref id="cube"/>
			</children>
		</component>

		<component id="elevation_p2">
			<transformation>
				<translate x="0" y="0" z="4" />
				<scale x="10" y="0.2" z="4" />
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<componentref id="cube"/>
			</children>
		</component>


		<!-- Cube -->
		<component id="cube">
			<transformation>
				<translate x="0" y="0" z="0" />
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<componentref id="faceCube1"/>
				<componentref id="faceCube2"/>
				<componentref id="faceCube3"/>
				<componentref id="faceCube4"/>
				<componentref id="faceCube5"/>
				<componentref id="faceCube6"/>
			</children>
		</component>

		<component id="faceCube1">
			<transformation>
				<translate x="0" y="0" z="0.5"/>
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<primitiveref id="rectangle" />
			</children>

		</component>

		<component id="faceCube2">
			<transformation>
				<translate x="0" y="0" z="-0.5"/>
				<rotate axis="y" angle="-180"/>
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<primitiveref id="rectangle"/>
			</children>

		</component>

		<component id="faceCube3">
			<transformation>
				<translate x="0.5" y="0" z="0"/>
