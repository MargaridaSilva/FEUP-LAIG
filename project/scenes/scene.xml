			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<primitiveref id="rectangle" />
			</children>

		</component>

		<component id="faceCube4">
			<transformation>
				<translate x="-0.5" y="0" z="0"/>
				<rotate axis="y" angle="-90"/>
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<primitiveref id="rectangle" />
			</children>

		</component>

		<component id="faceCube5">
			<transformation>
				<translate x="0" y="-0.5" z="0"/>
				<rotate axis="x" angle="90"/>
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<primitiveref id="rectangle" />
			</children>
		</component>

		<component id="faceCube6">
			<transformation>
				<translate x="0" y="0.5" z="0"/>
				<rotate axis="x" angle="-90"/>
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<primitiveref id="rectangle" />
			</children>
		</component>
	</components>
</yas>
