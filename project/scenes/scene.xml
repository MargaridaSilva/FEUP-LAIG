<yas>
	<scene root="scene" axis_length="5" />
	<views default="view1">
		<perspective id="view1" near="0.1" far="500" angle="0.4">
			<from x="15" y="15" z="15" />
			<to x="0" y="0" z="0" />
		</perspective>
		<perspective id="view2" near="0.1" far="500" angle="0.4">
			<from x="15" y="15" z="25" />
			<to x="0" y="0" z="0" />
		</perspective>
	</views>
	<ambient>
		<ambient r="0" g="0" b="0" a="1" />
		<background r="0" g="0" b="0" a="1" />
	</ambient>
	<lights>
		<omni id="light1" enabled="1">
			<location x="-40" y="40" z="-40" w="1" />
			<ambient r="0" g="0" b="0" a="1" />
			<diffuse r="1" g="1" b="1" a="1" />
			<specular r="1" g="1" b="1" a="1" />
		</omni>
		<omni id="light2" enabled="1">
			<location x="-40" y="40" z="40" w="1" />
			<ambient r="0" g="0" b="0" a="1" />
			<diffuse r="1" g="1" b="1" a="1" />
			<specular r="1" g="1" b="1" a="1" />
		</omni>
		<omni id="light3" enabled="1">
			<location x="40" y="40" z="-40" w="1" />
			<ambient r="0" g="0" b="0" a="1" />
			<diffuse r="1" g="1" b="1" a="1" />
			<specular r="1" g="1" b="1" a="1" />
		</omni>
		<omni id="light4" enabled="1">
			<location x="40" y="40" z="40" w="1" />
			<ambient r="0" g="0" b="0" a="1" />
			<diffuse r="1" g="1" b="1" a="1" />
			<specular r="1" g="1" b="1" a="1" />
		</omni>
	</lights>
	<textures>
		<texture id="bank" file="bank.jpg" />
	</textures>
	<materials>
		<material id="material1" shininess="0.5">
			<emission r="0" g="0" b="0" a="0" />
			<ambient r="0" g="0" b="0" a="0" />
			<diffuse r="0" g="0" b="0" a="0" />
			<specular r="0" g="0" b="0" a="0" />
		</material>
	</materials>
	<transformations>
		<transformation id="test">
			<translate x="0" y="0" z="0" />
		</transformation>
	</transformations>
	<primitives>
		<primitive id="rectangle">
			<rectangle x1="-0.5" y1="-0.5" x2="0.5" y2="0.5" />
		</primitive>
		<primitive id="triangle">
			<triangle x1="0" y1="0" z1="0" x2="0" y2="0" z2="0" x3="0" y3="0" z3="0" />
		</primitive>
		<primitive id="cylinder">
			<cylinder base="0" top="0" height="0" slices="0" stacks="0" />
		</primitive>
		<primitive id="sphere">
			<sphere radius="0" slices="0" stacks="0" />
		</primitive>
		<primitive id="torus">
			<torus inner="2" outer="3" slices="100" loops="500">
			</torus>
		</primitive>
	</primitives>
	<components>

		<component id="scene">
			<transformation>
				<translate x="0" y="0" z="0" />
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<componentref id="room"/>
				<componentref id="tables" />
				<componentref id="elevation" />
			</children>
		</component>


		<component id="room">
			<transformation>
				<scale x="40" y="20" z="50" />
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<componentref id="room_floor"/>
				<componentref id="room_back"/>
				<componentref id="room_side"/>
			</children>
		</component>


		<component id="room_floor">
			<transformation>
				<rotate axis="x" angle="-90"/>
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<primitiveref id="rectangle" />
			</children>
		</component>



		<component id="room_back">
			<transformation>
				<translate x="0" y="0.5" z="-0.5"/>
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<primitiveref id="rectangle" />
			</children>
		</component>


		<component id="room_side">
			<transformation>
				<translate x="-0.5" y="0.5" z="0"/>
				<rotate axis="y" angle="90"/>
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<primitiveref id="rectangle" />
			</children>
		</component>

		<component id="tables">
			<transformation>
				<translate x="0" y="0" z="0"/>
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<componentref id="table1"/>
				<componentref id="table2"/>
				<componentref id="table3"/>
				<componentref id="table4"/>
			</children>
		</component>

		<component id="table1">
			<transformation>
				<translate x="-15" y="0" z="0"/>
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<componentref id="table"/>
			</children>
		</component>


		<component id="table2">
			<transformation>
				<translate x="-5" y="0" z="0"/>
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<componentref id="table"/>
			</children>
		</component>

		<component id="table3">
			<transformation>
				<translate x="5" y="0" z="0"/>
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<componentref id="table"/>
			</children>
		</component>

		<component id="table4">
			<transformation>
				<translate x="15" y="0" z="0"/>
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<componentref id="table"/>
			</children>
		</component>

		<component id="table">
			<transformation>
				<translate x="0" y="1" z="0"/>
				<scale x="1" y="1" z="1" />
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<componentref id="top"/>
				<componentref id="leg1"/>
				<componentref id="leg2"/>
				<componentref id="leg3"/>
				<componentref id="leg4"/>
			</children>
		</component>


		<component id="top">
			<transformation>
				<translate x="0" y="1" z="0"/>
				<scale x="3" y="0.2" z="10" />
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<componentref id="cube"/>
			</children>
		</component>


		<component id="leg1">
			<transformation>
				<translate x="1" y="0" z="4.8"/>
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<componentref id="leg"/>
			</children>
		</component>

		<component id="leg2">
			<transformation>
				<translate x="-1" y="0" z="4.8"/>
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<componentref id="leg"/>
			</children>
		</component>

		<component id="leg3">
			<transformation>
				<translate x="1" y="0" z="-4.8"/>
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<componentref id="leg"/>
			</children>
		</component>

		<component id="leg4">
			<transformation>
				<translate x="-1" y="0" z="-4.8"/>
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
			<children>
				<componentref id="leg"/>
			</children>
		</component>

		<component id="leg">
			<transformation>
				<scale x="0.6" y="2" z="0.2" />
			</transformation>
			<materials>
				<material id="material1" />
			</materials>
			<texture id="bank" length_s="1.0" length_t="1.0" />
