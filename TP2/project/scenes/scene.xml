<yas>
	<scene root="scene" axis_length="5" />
	<views default="global">
		<perspective id="global" near="0.1" far="500" angle="35">
			<from x="5" y="5" z="5" />
			<to x="0" y="0" z="0" />
		</perspective>
	</views>
	<ambient>
		<ambient r="0.2" g="0.2" b="0.2" a="1" />
		<background r="0.05" g="0.05" b="0.1" a="1" />
	</ambient>
	<lights>
		<omni id="light1" enabled="1">
			<location x="10" y="20" z="0" w="1" />
			<ambient r="0.1" g="0.1" b="0.1" a="1" />
			<diffuse r="1.0" g="1.0" b="1.0" a="1" />
			<specular r="1.0" g="1.0" b="1.0" a="1" />
		</omni>
	</lights>
	<textures>
		<texture id="tableWood" file="scenes/images/wood.jpg" />
		<texture id="terrain_h" file="scenes/images/island-height.jpg" />
		<texture id="terrain_t" file="scenes/images/island.jpg" />
		
	</textures>
	<materials>
		<material id="white" shininess="0.5">
			<emission r="0" g="0" b="0" a="0" />
			<ambient r="1" g="1" b="1" a="1" />
			<diffuse r="1" g="1" b="1" a="1" />
			<specular r="1" g="1" b="1" a="1" />
		</material>
	</materials>
	<transformations>
		<transformation id="base">
			<translate x="0" y="0" z="0" />
		</transformation>
	</transformations>
	<animations>
		<linear id="example" span="0" >
			<controlpoint xx="0" yy="0" zz="0"/>
			<controlpoint xx="1" yy="1" zz="1"/>
		</linear>
		<circular id="example" span="0" center="0 0 0" radius="1" startang="1" rotang="1"/>
	</animations>
	<primitives>
		<primitive id="rectangle">
			<rectangle x1="0" y1="0" x2 ="1" y2="1" />
		</primitive>
		<primitive id="vehicle">
			<vehicle />
		</primitive>
		<primitive id="terrain">
			<terrain idtexture="terrain_t" idheightmap="terrain_h" parts="100" heightscale="20"/>
		</primitive>

		<primitive id="cylinder2">
			<cylinder2 base = "1.0" top = "1.0" height = "7.0" slices = "20" stacks = "5"/>
		</primitive>
	</primitives>
	<components>
		<component id="scene">
			<transformation>
				<transformationref id="base" />
			</transformation>
			<materials>
				<material id="white" />
			</materials>
			<texture id="none" length_s="1.0" length_t="1.0" />
			<children>
				<primitiveref id="cylinder2" />
			</children>
		</component>
	</components>
</yas>